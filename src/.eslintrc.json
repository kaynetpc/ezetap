{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console":"warn",
        "linebreak-style": "off",
        "new-cap": "off",
        "require-jsdoc": "off",
        "max-len": ["error", {"code": 150}],
        "valid-jsdoc": "off"
    }
}
